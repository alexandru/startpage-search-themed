{
	"manifest_version": 2,
	"name": "Startpage Search â€” Night Theme",
	"short_name": "startpage-search-night",
	"description": "Adds Startpage.com as a search engine, using the Night theme.",
	"version": "1.0.1",
	"icons": {
		"70": "icons/70x70.png",
		"144": "icons/144x144.png"
	},
	"chrome_settings_overrides": {
		"search_provider": {
			"favicon_url": "https://www.startpage.com/sp/spstatic/favicons/favicon-180x180.png",
			"is_default": true,
			"keyword": "!s",
			"name": "Startpage Search",
			"search_url": "https://www.startpage.com/do/dsearch?query={searchTerms}&cat=web&pl=opensearch&language=english&t=nite",
			"suggest_url": "https://www.startpage.com/cgi-bin/csuggest?query={searchTerms}&limit=10&lang=english&format=json"
		}
	}
}